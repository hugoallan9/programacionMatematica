#-*- coding:utf-8 -*-
class Calculadora():
    def sumar(self,a,b):
        return a + b

    def restar(self,a,b):
        return a - b

    def multiplicar(self, a,b):
        return a * b

    def dividir(self, a,b):
        if b == 0:
            return 'La divisi√≥n entre cero no es posible'
        else:
            return a / b
        

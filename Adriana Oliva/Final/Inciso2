# -*- coding: utf-8 -*-
import fractions

def phi(n):
    amount = 0

    for k in range(1, n + 1):
        if fractions.gcd(n, k) == 1:
            amount += 1

    return amount
a = []
for x in range(1,1000001):
	if (x / round(phi(x)))>4:
		a.append(x / round(phi(x)))
	else:
		continue
print 'Respuesta:', max(a)

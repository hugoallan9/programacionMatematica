# -*- coding: utf-8 -*-
def Primos(n):
	if n == 2: return [2]
	elif n < 2: return []
	s = range(3, n+1, 2)
	rc = n ** 0.5
	med = (n+1)/2-1
	i = 0
	m = 3
	while m <= rc:
		if s[i]:
			j = (m*m-3)/2
			s[j] = 0
			while j<med:
				s[j]=0
				j+=m 
		i = i + 1 
		m = 2*i+3 
	return[2]+[x for x in s if x]
print 'Respuesta:', Primos(200000)[10000]
#Encuentra el primo nÃºmero 10001

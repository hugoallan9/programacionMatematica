def Primo(n):
	if n == 2: return [2]
	elif n < 2: return []
	s = range(3, n+1, 2)
	rc = n ** 0.5
	med = (n+1)/2-1
	i=0
	m=3
	while m <= rc:
		if s [i]:
			j = (m*m-3)/2 
			s[j]=0
			while j < med:
				s[j] = 0
				j+= m 
		i = i + 1
		m = 2 * i + 3
	return [2] + [x for x in s if x]

list =[] 
for x in range(1000000):
	if (n / Primo(n)):
		list.append(x)
	else:
		continue
max(list)
